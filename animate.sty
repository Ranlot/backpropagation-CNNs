  %detects multipage PDF and corrects user supplied page range
  \def\@anim@checkmultipage#1#2{% #1: file base name, #2 user provided file ext
    \ifx\@empty#2\@empty%
      \IfFileExists{#1.pdf}{%
        \setboolean{@anim@multipage}{true}%
        \gdef\@anim@ext{.pdf}%
      }{%
        \IfFileExists{#1.jbig2}{%
          \setboolean{@anim@multipage}{true}%
          \gdef\@anim@ext{.jbig2}%
        }{%
          \IfFileExists{#1.jb2}{%
            \setboolean{@anim@multipage}{true}%
            \gdef\@anim@ext{.jb2}%
          }{}%
